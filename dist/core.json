{
  "facebook-pixel": {
    "name": "facebook-pixel",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "globalVariables": [
        "fbq"
      ]
    }
  },
  "framer": {
    "name": "framer",
    "categories": [
      "cms"
    ],
    "detectors": {
      "metaTagCheck": {
        "name": "generator",
        "contentRegex": "Framer"
      }
    }
  },
  "google-analytics": {
    "name": "google-analytics",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "requestUrlRegex": [
        "google-analytics\\.com/analytics\\.js",
        "google-analytics\\.com/collect",
        "analytics\\.google\\.com"
      ]
    }
  },
  "google-tag-manager": {
    "name": "google-tag-manager",
    "categories": [
      "tag-managers"
    ],
    "detectors": {
      "globalVariables": [
        "dataLayer",
        "google_tag_manager"
      ]
    }
  },
  "gtag": {
    "name": "gtag",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "globalVariables": [
        "gtag"
      ]
    }
  },
  "hotjar": {
    "name": "hotjar",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "globalVariables": [
        "hj",
        "hjSiteSettings"
      ]
    }
  },
  "klaviyo": {
    "name": "klaviyo",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "globalVariables": [
        "_learnq"
      ],
      "requestUrlRegex": [
        "a\\.klaviyo\\.com/media/js/analytics/analytics\\.js",
        "static\\.klaviyo\\.com/onsite/js/klaviyo\\.js",
        "static\\.klaviyo\\.com/onsite/js/klaviyo\\.min\\.js",
        "a\\.klaviyo\\.com/api/identify",
        "a\\.klaviyo\\.com/api/track",
        "fast\\.a\\.klaviyo\\.com",
        "static-forms\\.klaviyo\\.com"
      ]
    }
  },
  "next": {
    "name": "next",
    "categories": [
      "framework"
    ],
    "detectors": {
      "globalVariables": [
        "next"
      ],
      "htmlRegex": "(data-next-page|data-next-url|__NEXT_DATA__|__next)"
    }
  },
  "nuxt": {
    "name": "nuxt",
    "categories": [
      "framework"
    ],
    "detectors": {
      "globalVariables": [
        "__NUXT__",
        "$nuxt"
      ],
      "selectorExists": [
        "#__nuxt",
        "#__layout",
        "[data-n-head]",
        "[data-n-]"
      ]
    }
  },
  "react": {
    "name": "react",
    "categories": [
      "framework"
    ],
    "detectors": {
      "globalVariables": [
        "React",
        "__REACT_DEVTOOLS_GLOBAL_HOOK__"
      ],
      "selectorExists": [
        "div[data-reactroot]",
        "[data-reactid]",
        "noscript[data-reactroot]",
        "#react-root"
      ]
    }
  },
  "segment": {
    "name": "segment",
    "categories": [
      "analytics"
    ],
    "detectors": {
      "globalVariables": [
        "analytics",
        "analytics.initialize",
        "analytics.invoked"
      ],
      "selectorExists": [
        "script[src*='cdn.segment.com/analytics.js']",
        "script[src*='cdn.segment.com/analytics.min.js']"
      ]
    }
  },
  "shopify": {
    "name": "shopify",
    "categories": [
      "cms",
      "ecommerce"
    ],
    "detectors": {
      "metaTagCheck": {
        "name": "shopify-checkout-api-token",
        "contentRegex": ".*"
      },
      "globalVariables": [
        "Shopify",
        "ShopifyAnalytics"
      ]
    }
  },
  "squarespace": {
    "name": "squarespace",
    "categories": [
      "cms"
    ],
    "detectors": {
      "globalVariables": [
        "Squarespace"
      ],
      "metaTagCheck": {
        "name": "generator",
        "contentRegex": "Squarespace"
      }
    },
    "themeDetection": {
      "method": "global",
      "globalPath": "Static.SQUARESPACE_CONTEXT.templateName"
    }
  },
  "statamic": {
    "name": "statamic",
    "categories": [
      "cms"
    ],
    "detectors": {
      "cookieCheck": [
        "statamic_session"
      ]
    }
  },
  "svelte": {
    "name": "svelte",
    "categories": [
      "framework"
    ],
    "detectors": {
      "globalVariables": [
        "__svelte"
      ]
    }
  },
  "vue": {
    "name": "vue",
    "categories": [
      "framework"
    ],
    "detectors": {
      "globalVariables": [
        "Vue",
        "__VUE_DEVTOOLS_GLOBAL_HOOK__",
        "VueRouter"
      ],
      "selectorExists": [
        "[data-v-]",
        "#app[data-v-app]",
        "[data-server-rendered='true']"
      ]
    }
  },
  "webflow": {
    "name": "webflow",
    "categories": [
      "cms"
    ],
    "detectors": {
      "globalVariables": [
        "Webflow"
      ],
      "metaTagCheck": {
        "name": "generator",
        "contentRegex": "Webflow"
      },
      "selectorExists": [
        ".w-webflow-badge"
      ]
    },
    "themeDetection": {
      "cssLinkRegex": "css/([\\w-]+)\\.webflow\\.css"
    }
  },
  "wix": {
    "name": "wix",
    "categories": [
      "cms"
    ],
    "detectors": {
      "globalVariables": [
        "wixBiSession",
        "wixSdk"
      ],
      "metaTagCheck": {
        "name": "generator",
        "contentRegex": "Wix\\.com"
      },
      "selectorExists": [
        "div[class*='wixui-']"
      ]
    }
  },
  "wordpress": {
    "name": "wordpress",
    "categories": [
      "cms"
    ],
    "detectors": {
      "metaTagCheck": {
        "name": "generator",
        "contentRegex": "WordPress"
      },
      "selectorExists": [
        "script[src*='wp-includes']",
        "body.wp-",
        "#wpadminbar"
      ],
      "globalVariables": [
        "wp",
        "wpApiSettings",
        "wc"
      ],
      "themeDetection": {
        "stylesheetRegex": "wp-content\\/themes\\/([-\\w]+)",
        "bodyClassRegex": "theme-([-\\w]+)",
        "cssIdentifierRegex": "Theme Name:\\s*([^\\n]+)"
      }
    }
  }
}